{
	"version": "0.4.4",
	"main": "service.manifest.json",
	"modules": [
		{
			"resources": [
				"./",
				"./index.service.test.html",
				"./bootstrap.html",
				"./index.html",
				"./service.manifest.json",

				"./settings.html",

				"/shared/vendor/stylus.min.js",
				"/shared/vendor/materialize.min.js",

				"/shared/vendor/localforage.min.js",
				"/shared/vendor/json5v-2.0.0.min.js",

				"/shared/fonts/flUhRq6tzZclQEJ-Vdg-IuiaDsNcIhQ8tQ.woff2",
				"/shared/fonts/seti.woff2",
				"/shared/fonts/codicon.ttf",
				"/shared/fonts/Apl385.ttf",

				"/shared/css/xterm.css",
				"/shared/css/seti-icons.css",
				"/shared/css/material-icons.css",
				"/shared/css/materialize.min.css"
			]
		},
		"NOTE: -- TERMINAL UI /START --",
		{
			"route": "./_/modules/terminal/index.html",
			"source": "terminal/terminal.html"
		},
		{
			"route": "./_/modules/terminal/terminal.css",
			"source": "terminal/terminal.css"
		},
		{
			"route": "./_/modules/terminal/terminal.js",
			"source": "terminal/terminal.js"
		},
		{
			"route": "./_/modules/terminal/terminal.comm.js",
			"source": "terminal/terminal.comm.js"
		},
		{
			"route": "./_/modules/terminal/terminal.git.js",
			"source": "terminal/terminal.git.js"
		},
		{
			"route": "./_/modules/terminal/terminal.history.js",
			"source": "terminal/terminal.history.js"
		},
		{
			"route": "./_/modules/terminal/terminal.keys.js",
			"source": "terminal/terminal.keys.js"
		},
		{
			"route": "./_/modules/terminal/terminal.lib.js",
			"source": "terminal/terminal.lib.js"
		},
		{
			"route": "./_/modules/terminal/terminal.ops.js",
			"source": "terminal/terminal.ops.js"
		},
		{
			"route": "./_/modules/terminal/terminal.ops.dynamic.js",
			"source": "terminal/terminal.ops.dynamic.js"
		},
		{
			"route": "./_/modules/terminal/terminal.utils.js",
			"source": "terminal/terminal.utils.js"
		},
		{
			"route": "./_/modules/terminal/terminal.watch.js",
			"source": "terminal/terminal.watch.js"
		},
		{
			"route": "./_/modules/terminal/terminal.xterm.js",
			"source": "terminal/terminal.xterm.js"
		},
		{
			"route": "./_/modules/terminal/bin/cat.js",
			"source": "terminal/bin/cat.js"
		},
		{
			"route": "./_/modules/terminal/bin/node.js",
			"source": "terminal/bin/node.js"
		},
		{
			"route": "./_/modules/terminal/bin/preview.js",
			"source": "terminal/bin/preview.js"
		},
		{
			"route": "./_/modules/terminal/bin/ls.js",
			"source": "terminal/bin/ls.js"
		},
		"NOTE: -- TERMINAL UI /END --",

		"NOTE: modules",
		{
			"route": "./_/modules/Panes.mjs",
			"type": "domComponent",
			"source": "modules/Panes.mjs"
		},
		{
			"route": "./_/modules/StatusBar.mjs",
			"type": "domComponent",
			"source": "modules/StatusBar.mjs"
		},
		{
			"route": "./_/modules/ActionBar.mjs",
			"type": "domComponent",
			"source": "modules/ActionBar.mjs"
		},
		{
			"route": "./_/modules/TreeView.mjs",
			"type": "domComponent",
			"source": "modules/TreeView.mjs",
			"resources": ["/shared/modules/TreeView.mjs"]
		},
		{
			"route": "./_/modules/LoaderBar.mjs",
			"type": "domComponent",
			"source": "modules/LoaderBar.mjs"
		},
		{
			"route": "./_/modules/Terminal.mjs",
			"type": "domComponent",
			"source": "modules/Terminal.mjs",
			"resources": [
				"/shared/vendor/xterm.min.js",
				"/shared/vendor/xterm-addon-fit.js"
			]
		},
		{
			"route": "./_/modules/HotKeys.mjs",
			"type": "domComponent",
			"source": "modules/HotKeys.mjs"
		},
		{
			"route": "./_/modules/Services.mjs",
			"type": "domComponent",
			"source": "modules/Services.mjs"
		},
		{
			"route": "./_/modules/Editor.mjs",
			"type": "domComponent",
			"source": "modules/Editor.mjs",
			"resources": [
				"/shared/modules/editor.mjs",
				"/shared/css/codemirror.css",
				"/shared/css/bespin.css",
				"/shared/css/vscode.codemirror.css",

				"/shared/vendor/codemirror.js",
				"/shared/vendor/codemirror/addon.bundle.js",
				"/shared/vendor/codemirror/mode.bundle.js"
			]
		},
		{
			"route": "./_/modules/ContextMenu.mjs",
			"type": "domComponent",
			"source": "modules/ContextMenu.mjs"
		},
		{
			"route": "./_/modules/Operations.mjs",
			"type": "domComponent",
			"source": "modules/Operations.mjs"
		},

		"NOTE: should be a handlers in service worker context",
		{
			"route": "./_/modules/statusBarEvents.mjs",
			"source": "modules/statusBarEvents.mjs",
			"handler": null
		},
		{
			"route": "./_/modules/treeEvents.mjs",
			"source": "modules/treeEvents.mjs",
			"handler": null
		},
		{
			"route": "./_/modules/loaderBarEvents.mjs",
			"source": "modules/loaderBarEvents.mjs",
			"handler": null
		},
		{
			"route": "./_/modules/terminalEvents.mjs",
			"source": "modules/terminalEvents.mjs",
			"handler": null
		},
		{
			"route": "./_/modules/servicesEvents.mjs",
			"source": "modules/servicesEvents.mjs",
			"handler": null
		},
		{
			"route": "./_/modules/templatesEvents.mjs",
			"source": "modules/templatesEvents.mjs",
			"handler": null
		},
		{
			"route": "./_/modules/editorEvents.mjs",
			"source": "modules/editorEvents.mjs",
			"handler": null
		},
		{
			"route": "./_/modules/editorTabsEvents.mjs",
			"source": "modules/editorTabsEvents.mjs",
			"handler": null
		},
		{
			"route": "./_/modules/operationsEvents.mjs",
			"source": "modules/operationsEvents.mjs",
			"handler": null
		},

		"NOTE: should be compiled in to each module",
		{
			"route": "./_/modules/Listeners.mjs",
			"source": "modules/Listeners.mjs",
			"handler": null
		},
		{
			"route": "./_/modules/Templates.mjs",
			"source": "modules/Templates.mjs",
			"handler": null
		},
		{
			"route": "./_/modules/EditorTabs.mjs",
			"source": "modules/EditorTabs.mjs"
		},
		{
			"route": "./_/modules/motd.mjs",
			"source": "modules/motd.mjs",
			"handler": null
		},
		{
			"route": "./_/modules/state.mjs",
			"source": "modules/state.mjs",
			"handler": null
		},
		{
			"route": "./_/modules/ExternalState.mjs",
			"source": "modules/ExternalState.mjs",
			"handler": null
		},
		{
			"route": "./_/modules/operationsManagement.mjs",
			"source": "modules/operationsManagement.mjs",
			"handler": null
		},
		{
			"route": "./_/modules/operationsService.mjs",
			"source": "modules/operationsService.mjs",
			"handler": null
		},
		{
			"route": "./_/modules/Types.mjs",
			"source": "modules/Types.mjs",
			"handler": null
		},
		{
			"route": "/shared/modules/utilities.mjs",
			"source": "/shared/modules/utilities.mjs",
			"handler": null
		},
		{
			"route": "/shared/icons/seti/ext.json.mjs",
			"source": "/shared/icons/seti/ext.json.mjs",
			"handler": null
		},

		"NOTE: should be bundled and minified?",
		{
			"route": "./index.css",
			"source": "index.colors.css",
			"include": [
				"/index.css",
				"/shared/css/xterm.css",
				"/shared/css/seti-icons.css",
				"/shared/css/material-icons.css",
				"/shared/css/materialize.min.css"
			]
		},

		"NOTE: should be bundled and minified?",
		{
			"route": "./colors.css",
			"source": "index.colors.css"
		},

		"NOTE: external handlers",
		{
			"route": "/service/(.*)",
			"type": "fetch",
			"handler": "./modules/service-worker.handler.js"
		},
		{
			"route": "/manage/(.*)",
			"type": "fetch",
			"handler": "./modules/service-worker.handler.js"
		},
		{
			"route": "/monitor/(.*)",
			"type": "fetch",
			"handler": "./modules/service-worker.handler.js"
		},
		{
			"route": "/persist/(.*)",
			"type": "fetch",
			"handler": "./modules/service-worker.handler.js"
		},

		"NOTE: end modules"
	]
}
